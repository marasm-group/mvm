;; debIO -- debug input/output device module
;;
#json
{
    "author":"SR3u",
    "dependencies":[],
    "init":"$common_init"
}
#end
$reverseStack ; reverces first n stacks elements order
var n
pop n
add n n 1
var tmp[n]
var tmp_n
mov tmp_n n
@common_reverseStack_input_loop
sub n n 1
pop tmp[n]
jnz n @common_reverseStack_input_loop
mov n tmp_n
@common_reverseStack_output_loop
sub n n 1
push tmp[n]
jnz n @common_reverseStack_output_loop
ret
$common_init
ret